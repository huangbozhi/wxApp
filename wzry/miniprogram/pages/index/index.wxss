.container{
    width: 100%;
    height: 100%;
}
.form-box{
    display: block;
    padding-left: 30rpx;
    /* border: 1px solid #000; */
    width: 100%;
    /* height: 600rpx; */
    position: relative;
    /* margin-left: 35%; */
    margin-top: 48%;
}
.Img{
    display: block;
    position: absolute;
    width: 150rpx;
    height: 150rpx;
    top: 200rpx;
}
.Img image{
    position: relative;
    left: 50%;
    top: 30%;
    transform: translate(-50%,-50%);
    background-size: contain;
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}
.username{
    position: relative;
    border: 1px solid #ededed;
}
.password{
    position: relative;
    border: 1px solid #ededed;
    margin-top: 20rpx;
}
input{
    height: 90rpx;
    border-bottom: 1rpx solid #ededed;
    font-size: 28rpx;
    color: #000;
}
.btn{
    display: flex;
    margin-left: 40rpx;
}
.login{
    margin: 30rpx 30rpx 0 0;
    color: #ffffff;
    background-color: #009900;
    width: 90%;
}
.register{
    margin: 30rpx 30rpx 0 0;
    color: #ffffff;
    background-color: #0000ff;
    width: 90%;
}
.imgs{
    display: flex;
    position: relative;
    border: 1px solid #000;
    margin-top: 20rpx;
}